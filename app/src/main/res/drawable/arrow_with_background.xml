<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background with corner radius -->
    <item>
        <shape>
            <solid android:color="@color/backgroundWhiteThemeEnterColor" />
            <corners android:radius="80dp" />
            <stroke android:width="1dp" android:color="@color/backgroundWhiteThemeEnterColor" />
        </shape>
    </item>

    <!-- Arrow icon -->
    <item android:drawable="@drawable/enter_key_arrow" android:gravity="center" />
</layer-list>
